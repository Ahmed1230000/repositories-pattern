{
    "name": "ahmedmahmoud/repository-pattern",
    "description": "A Laravel package to implement the Repository Pattern",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mahmoud",
            "email": "ahmed.jokerjoo1230@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Packages\\AhmedMahmoud\\RepositoryPattern\\": "src/",
            "Packages\\AhmedMahmoud\\RepositoryPattern\\Src\\Helpers\\": "src/Helpers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Packages\\AhmedMahmoud\\RepositoryPattern\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Packages\\AhmedMahmoud\\RepositoryPattern\\RepositoryServiceProvider"
            ]
        }
    }
}