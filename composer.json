{
    "name": "ahmedmahmoud/repository-pattern",
    "description": "A Laravel package to implement the Repository Pattern",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mahmoud",
            "email": "ahmed.jokerjoo1230@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Ahmed1230000/repositories-pattern.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "autoload": {
        "psr-4": {
            "Packages\\AhmedMahmoud\\RepositoryPattern\\": "src/",
            "Packages\\AhmedMahmoud\\RepositoryPattern\\Src\\Helpers\\": "src/Helpers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Packages\\AhmedMahmoud\\RepositoryPattern\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Packages\\AhmedMahmoud\\RepositoryPattern\\RepositoryServiceProvider"
            ]
        }
    }
}
